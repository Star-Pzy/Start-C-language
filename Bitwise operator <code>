#include <stdio.h>

int main()
{
	int a = 3;
	int b = 5;
	int c = 0;
	int d = 0;
	int e = 0;
	c = a & b;
	d = a | b;
	e = a ^ b;
	printf("%d\n", c);   //1
	printf("%d\n", d);   //7
	printf("%d\n", e);   //6
	return 0;
}

//& | ^都是对补码进行计算。
//补码：
//a:00000000000000000000000000000011
//b:00000000000000000000000000000101
//c:00000000000000000000000000000001
//d:00000000000000000000000000000111
//e:00000000000000000000000000000110
//^是相异为1
